/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel, out=ns);
    And(a=a, b=ns, out=v1);
    And(a=sel, b=b, out=v2);
    Or(a=v1, b=v2, out=out);
}